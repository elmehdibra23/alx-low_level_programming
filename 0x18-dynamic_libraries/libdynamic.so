#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int _putchar(char c) {
    return putchar(c);
}

int _islower(int c) {
    return (c >= 'a' && c <= 'z');
}

int _isalpha(int c) {
    return (_islower(c) || _isupper(c));
}

int _abs(int n) {
    return (n < 0) ? -n : n;
}

int _isupper(int c) {
    return (c >= 'A' && c <= 'Z');
}

int _isdigit(int c) {
    return (c >= '0' && c <= '9');
}

int _strlen(char *s) {
    int len = 0;
    while (*s) {
        len++;
        s++;
    }
    return len;
}

void _puts(char *s) {
    while (*s) {
        _putchar(*s);
        s++;
    }
    _putchar('\n');
}

char *_strcpy(char *dest, char *src) {
    char *tmp = dest;
    while (*src) {
        *dest = *src;
        dest++;
        src++;
    }
    *dest = '\0';
    return tmp;
}

int _atoi(char *s) {
    int sign = 1, num = 0;

    while (*s && (*s == ' ' || *s == '\t' || *s == '\n' || *s == '\r')) {
        s++;
    }

    if (*s == '-') {
        sign = -1;
        s++;
    }

    if (*s == '+') {
        s++;
    }

    while (*s && _isdigit(*s)) {
        num = num * 10 + (*s - '0');
        if (num < 0 || (num * sign) < INT_MIN || (num * sign) > INT_MAX) {
            return 0;
        }
        s++;
    }

    return num * sign;
}

char *_strcat(char *dest, char *src) {
    char *tmp = dest;
    while (*dest) {
        dest++;
    }
    while (*src) {
        *dest = *src;
        dest++;
        src++;
    }
    *dest = '\0';
    return tmp;
}

char *_strncat(char *dest, char *src, int n) {
    char *tmp = dest;
    int i = 0;
    while (*dest) {
        dest++;
    }
    while (*src && i < n) {
        *dest = *src;
        dest++;
        src++;
        i++;
    }
    if (i == n) {
        *dest = '\0';
    }
    return tmp;
}

